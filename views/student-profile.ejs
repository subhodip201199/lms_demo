    $(document).ready(function () {
    
        $(".sendprint").click(function() {

            
        
            var print = [];
            var others_print = [];
            var checkboxes_print = document.getElementsByClassName("checkbox_print");
            var checkboxes2_print = document.getElementsByClassName("checkbox_print2");
        
        
            for (var i = 0; i <= checkboxes_print.length-1; i++) {
                if (checkboxes_print[i].type === "checkbox" && checkboxes_print[i].checked) {
                    print.push(checkboxes_print[i].value);
                }
            }
            for (var i = 0; i <= checkboxes2_print.length-1; i++) {
                if (checkboxes2_print[i].type === "checkbox" && checkboxes2_print[i].checked) {
                    others_print.push(checkboxes2_print[i].value);
                }
            }
        
            print_link = "/payment_history/bill_print?emails=test&emails=test&others=test&others=test";
        
            for(var i = 0; i <= print.length-1; i++){
                print_link += "&emails=" + print[i];
                
            }
            for(var i = 0; i <= others_print.length-1; i++){
                print_link += "&others=" + others_print[i];
                
            }
            //console.log(emails);
            console.log(print_link);

            if(!print[0] && !others_print[0]){
                alert("Please select a item!")
            } else{
                $(this).attr("href", print_link);
            }
        
            
            
            
        });
    });

