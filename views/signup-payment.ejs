                                                    <label class="btn btn-outline-secondary">
                                                        <input type="radio"
                                                               id="payment_pp"
                                                               name="payment_type"
                                                               value="pp"
                                                               aria-checked="true" /> PayPal
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
